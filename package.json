  "dependencies": {
    "bowser": "^2.11.0",
    "ua-parser-js": "^1.0.2",
    "web-vitals": "^2.1.4",
    "@monitor/core": "1.0.0",
    "@monitor/browser": "1.0.0",
    "@monitor/types": "1.0.0",
    "@monitor/utils": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@commitlint/cli": "^17.0.2",
