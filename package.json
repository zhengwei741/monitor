    "rollup-plugin-visualizer": "^5.7.1",
    "semver": "^7.3.7",
    "ts-jest": "^28.0.7",
    "tslib": "^2.4.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
